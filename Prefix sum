problem link :https://www.codechef.com/LTIME100B/problems/UNQEQ

solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    if(n<4){
	        cout<<"NO"<<endl;
	    }
	    
	    else{
	        
	        vector<int> s1;
	        vector<int> s2;
	        int k=1,j=n;
	        
	        if((n/2) % 2 !=0){
	            cout<<"NO"<<endl;
	        }
	        else{
	            cout<<"YES"<<endl;
	        for(int i =0;i<n/2;i++){
	            
	            if(i %2 ==0){
	            s1.push_back(k);
	            s1.push_back(j);
	            }
	            else{
	                s2.push_back(k);
	                s2.push_back(j);
	            }
	            k++;
	            j--;
	        }
	        sort(s1.begin(),s1.end());
	        sort(s2.begin(),s2.end());
	        
	        for(int i =0;i<s1.size();i++){
	            cout<<s1[i]<<" ";
	        }
	        cout<<endl;
	        
	        for(int i =0;i<s2.size();i++){
	            cout<<s2[i]<<" ";
	        }
	        
	        }
	        
	        
	    }
	}
	return 0;
}
