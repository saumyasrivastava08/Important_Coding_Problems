#include<iostream>
using namespace std;

int parallel_distance ( int car1[], int car2[] )
{
  int distance_traveled_car1 = 0, distance_traveled_car2 = 0, parallel =0;
  for(int i = 0;i < 5; i++)
  {
    distance_traveled_car1 += car1[i];
    distance_traveled_car2 += car2[i];
     
    if((distance_traveled_car1 == distance_traveled_car2) && (car1[i] == car2[i]))
    {
      parallel += car1[i];
    }
    
  } 
  return parallel;
}

int main() 
{

    int a[5] = {1,2,3,2,4};
    int b[5] = {2,1,3,1,4};
  
    cout << parallel_distance( a, b );
    return 0;
}
